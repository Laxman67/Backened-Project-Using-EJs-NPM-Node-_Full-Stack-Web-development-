<%- include('partials/header') -%>

<!-- // extra hypen - symbol to include file  -->

<h1>Compose</h1>


<form action="/compose" method="post">
  
  <div class="form-group">
    <label for="title_data">Title</label>
    <input type="text" class="form-control" name="title_data" id="title_data" placeholder="">
  </div>

  <div class="form-group">
    <label for="post_data">Post</label>
    <textarea  name="post_data" class="form-control" id="textarea_data" rows="3"></textarea>
  </div>

  <button type="submit" class="btn btn-primary">Publish</button>
</form>

<%- include('partials/footer') -%>
